<% provide(:title, '新規登録') %>
<% provide(:button_text, '登録する') %>
<div class="form">
  <h2>新規登録</h2>
  <%= render 'form' %>
	<%= link_to "Googleから新規登録", "/auth/google_oauth2", method: :post %>
  <div class ="other">
    <p>既に登録済みの方はログインしてください</p>
    <%= link_to (login_path) do %>
      <div class= "form-btn btn-navy">ログイン</div>
    <% end %>
  </div>
</div>
